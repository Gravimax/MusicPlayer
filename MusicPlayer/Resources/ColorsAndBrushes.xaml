<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MusicPlayer.Resources">
    <SolidColorBrush x:Key="yellowBrush" Color="#FFFFB03B" />
    <SolidColorBrush x:Key="oragneBrush" Color="#FFB64926" />
    <SolidColorBrush x:Key="redBrush" Color="#FF8E2800" />
    <SolidColorBrush x:Key="medCharcoleBrush" Color="#3D3D3D" />
    <SolidColorBrush x:Key="lightCharcoleBrush" Color="#686868" />
    <RadialGradientBrush x:Key="blueGreyGradient">
        <GradientStop Offset="0" Color="#525252" />
        <GradientStop Offset="1" Color="#3d72b4" />
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="GreyGradient">
        <GradientStop Offset="0" Color="LightGray" />
        <GradientStop Offset="1" Color="DarkGray" />
    </RadialGradientBrush>
</ResourceDictionary>